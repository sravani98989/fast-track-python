        "2.calculating sum and average"
      ],
      "metadata": {
        "id": "XCQiB5FrZVzS"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "sum=0.0\n",
        "nocount=0\n",
        "\n",
        "while True:\n",
        "     number= float(input(\"enter the number:\"))\n",
        "     sum+=number\n",
        "     nocount+=1\n",
        "\n",
        "choice = input(\"Add another number ? (y/\\n) : \")\n",
        "if choice.casefold() =='\n",
        "break\n",
        "average = sum/nocount\n",
        "\n",
        "print(f\"sum: {sum}\")\n",
        "print(f\"average:{average}\")\n",
        "\n"
      ],
